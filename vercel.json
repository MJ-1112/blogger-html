{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/create",
        "dest": "/public/create.html"
      },
      {
        "src": "/view",
        "dest": "/public/view.html"
      },
      {
        "src": "/about",
        "dest": "/public/about.html"
      },
      {
        "src": "/(.+)",
        "dest": "/public/$1"
      }
    ]
  }